# CAEN
Host caen
	HostName login.engin.umich.edu
	User jphow
	Compression yes
	ForwardX11 yes
	ControlMaster auto
	ControlPath ~/.ssh/master-%r@%h:%p
	ControlPersist 5m
Host pi
	HostName 192.168.1.11
	User pi
	Compression yes
	ControlMaster auto
	ControlPath ~/.ssh/master-%r@%h:%p
	ControlPersist 5m
Host aws
	HostName ec2-18-217-80-176.us-east-2.compute.amazonaws.com
	User ubuntu
	IdentityFile ~/.ssh/jphow-aws-instance.pem
	Compression yes
	ControlMaster auto
	ControlPath ~/.ssh/master-%r@%h:%p
	ControlPersist 5m
